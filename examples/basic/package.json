    "@emurgo/cardano-serialization-lib-nodejs": "^10.1.0",
    "bip39": "^3.0.4"
  },
  "devDependencies": {
    "@types/bip39": "^3.0.0",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "typescript": "^4.7.2"
  }
