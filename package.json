{
  "name": "@blockfrost/blockfrost-js",
  "version": "0.8.0",
  "description": "A JavaScript/TypeScript SDK for interacting with the https://blockfrost.io API",
  "files": [
    "lib/**/*.js",
    "lib/**/*.ts"
  ],
  "keywords": [
    "blockfrost",
    "cardano",
    "cardano api"
  ],
  "license": "Apache-2.0",
  "author": "blockfrost.io",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "prepublishOnly": "yarn build",
    "clean": "rimraf lib",
    "generate-types": "openapi-typescript ./node_modules/@blockfrost/openapi/swagger.yaml --output ./src/types/OpenApi.ts",
    "lint": "eslint ./src/**/*.ts",
    "run-example:api": "ts-node-dev --transpile-only ./example/api.ts",
    "run-example:ipfs": "ts-node-dev --transpile-only ./example/ipfs.ts",
    "test": "jest -c ./jest.config.js"
  },
  "dependencies": {
    "@blockfrost/openapi": "^0.1.23",
    "@emurgo/cardano-serialization-lib-nodejs": "^7.1.0",
    "@swc-node/jest": "^1.3.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "ipfs-http-client": "^50.1.2",
    "retry-axios": "^2.4.0",
    "rimraf": "^3.0.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "^27.0.6",
    "jest-extended": "^0.11.5",
    "openapi-typescript": "^4.0.1",
    "prettier": "2.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsc-watch": "^4.4.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.21.0",
    "typescript": "4.3.4"
  },
  "engines": {
    "node": ">=14"
  }
}
