{
  "name": "@blockfrost/blockfrost-js",
  "version": "4.1.0",
  "description": "A JavaScript/TypeScript SDK for interacting with the https://blockfrost.io API",
  "keywords": [
    "blockfrost",
    "cardano",
    "cardano api"
  ],
  "license": "Apache-2.0",
  "author": "blockfrost.io",
  "repository": "blockfrost/blockfrost-js",
  "main": "lib/index.js",
  "files": [
    "lib/**/*.js",
    "lib/**/*.ts"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf lib",
    "generate-types": "openapi-typescript https://raw.githubusercontent.com/blockfrost/openapi/master/openapi.yaml --output ./src/types/OpenApi.ts",
    "lint": "eslint ./src/**/*.ts",
    "prepublishOnly": "yarn build",
    "run-example:api": "ts-node-dev --transpile-only ./examples/basic/api.ts",
    "run-example:ipfs": "ts-node-dev --transpile-only ./examples/basic/ipfs.ts",
    "test": "yarn run-s 'test:*'",
    "test:unit": "jest -c ./jest.config.js --runInBand",
    "test:badges": "make-coverage-badge --output-path ./docs/badge-coverage.svg",
    "docs": "yarn typedoc ./src/index.ts --theme ./node_modules/typedoc-github-wiki-theme/dist"
  },
  "dependencies": {
    "@emurgo/cardano-serialization-lib-nodejs": "^10.0.4",
    "@emurgo/cip14-js": "3.0.1",
    "bottleneck": "^2.19.5",
    "form-data": "^4.0.0",
    "got": "^12.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@jest/globals": "^27.5.1",
    "@swc-node/jest": "^1.4.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.22",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "make-coverage-badge": "^1.2.0",
    "nock": "^13.2.4",
    "openapi-typescript": "^5.2.0",
    "prettier": "2.6.0",
    "rimraf": "^3.0.2",
    "serialize-error": "^9.1.1",
    "ts-jest": "^27.1.3",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.22.13",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.14",
    "typescript": "4.6.2",
    "yarn-run-all": "^3.1.1"
  },
  "engines": {
    "node": ">=14"
  }
}
