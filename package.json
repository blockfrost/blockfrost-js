{
  "name": "@blockfrost/blockfrost-js",
  "version": "0.3.1",
  "description": "A JavaScript/TypeScript SDK for interacting with the https://blockfrost.io API",
  "files": [
    "lib/**/*.js",
    "lib/**/*.ts"
  ],
  "keywords": [
    "blockfrost",
    "cardano",
    "cardano api"
  ],
  "license": "Apache-2.0",
  "author": "blockfrost.io",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "prepublishOnly": "yarn build",
    "clean": "rimraf lib",
    "generate-types": "pnpify openapi-typescript ./node_modules/@blockfrost/openapi/swagger.yaml --output ./src/types/OpenApi.ts",
    "lint": "eslint ./src/**/*.ts",
    "run-example": "ts-node-dev --transpile-only ./example/index.ts",
    "test": "jest --setupFiles dotenv/config"
  },
  "dependencies": {
    "@blockfrost/openapi": "0.1.15",
    "@yarnpkg/pnpify": "^2.4.0",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "dotenv": "^8.2.0",
    "rimraf": "^3.0.2",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/url-join": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "^26.6.3",
    "openapi-typescript": "^3.0.1",
    "prettier": "2.2.0",
    "ts-jest": "^26.4.4",
    "ts-node": "9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsc-watch": "^4.2.8",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.20.35",
    "typescript": "4.2.3"
  },
  "engines": {
    "node": ">=14"
  }
}
